{
  "compilerOptions": {
    "module": "ESNext",
    "target": "ES2022",
    "strict": true,
    "noImplicitAny": true,
    "strictNullChecks": true,
    "esModuleInterop": true,
    "declaration": true,
    "moduleResolution": "Bundler",
    "removeComments": false,
    "lib": ["ES2022"],
    "allowJs": true,
    "typeRoots": ["node_modules/@types", "./"],
    "baseUrl": "./",
    "paths": {
      "@/*": ["services/*"]
    }
  },
  "exclude": ["node_modules"],
  "include": ["cohere.ts"],
}
